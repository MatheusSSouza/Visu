

package mateface;

/**Classe de v√©rtices da malha */
public class mfVertex extends mfMalha{

    private float[] coords = new float[3];
  private int mae;

  public void setCoords(float[] c){
    for( int i = 0; i<3; i++){
      coords[i]=c[i];
    }}

  public float getCoords(int i){
    return coords[i];}



  public void setMae(int i){
    mae=i;}

  public int getMae(){
    return mae;}

}
